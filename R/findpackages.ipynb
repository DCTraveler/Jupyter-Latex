   "source": [
    "## Find all .libPaths() entries a package is found:\n",
    "findPkgAll <- function(pkg)\n",
    "  unlist(lapply(.libPaths(), function(lib)\n",
    "           find.package(pkg, lib, quiet=TRUE, verbose=FALSE)))"
   ]
